inherit distutils

DESCRIPTION="Finds common code segments in source trees"
HOMEPAGE="http://www.catb.org/~esr/${PN}/"
SRC_URI="http://www.catb.org/~esr/${PN}/${P}.tar.gz"

DIFF_EXCLUDES="test"

src_compile() {
	lndirs
	cd ${B}
	cygmake
}

src_install() {
	cd ${B}
	dobin ${PN}.exe filterator
	doman ${PN}.1
	distutils_install
}
